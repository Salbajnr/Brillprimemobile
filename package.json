    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.60.5",
    "@types/leaflet": "^1.9.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
